{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowS3ReadWriteBucketGigadbDatasetFiles",
      "Effect": "Allow",
      "Action": [
        "s3:ReplicateObject",
        "s3:PutObject",
        "s3:GetObjectAcl",
        "s3:GetObject",
        "s3:ListBucket",
        "s3:DeleteObject",
        "s3:GetBucketLocation",
        "s3:PutObjectAcl"
      ],
      "Resource": [
        "arn:aws:s3:::gigadb-datasetfiles-backup/*",
        "arn:aws:s3:::gigadb-datasetfiles-backup"
      ]
    }
  ]
}